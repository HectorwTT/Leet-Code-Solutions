public class Solution {
    public bool ScoreBalance(string s) {
      int [] sum=new int[s.Length];
      int total=0;
      for(int i=0;i<s.Length;i++){
       char ch=s[i];
       int value=ch-96; 
       total+=value;
       sum[i]=total;
      }
      for(int i=0;i<s.Length;i++){
       int prefix=sum[i];
       int suffix=total-sum[i];
       if(prefix==suffix) return true;
      }   
      return false;      
    }
}
