class Solution {
    public int maxOperations(String s) {
     int answer=0;
     int numberOfOnes=0;
     for(int i=0;i<s.length();i++){
      char bit=s.charAt(i);
      if(bit=='1') numberOfOnes++;
      else{
       if(i>0 && s.charAt(i-1)=='1') answer+=numberOfOnes;
      }   
     }
     return answer;      
    }
}
