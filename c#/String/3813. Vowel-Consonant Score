public class Solution {
    public int VowelConsonantScore(string s) {
     int numberOfVowels=0;
      int numberOfConsonants=0;
      for(int i=0;i<s.Length;i++){
       char ch=s[i];
       if(ch<97 || ch>122)continue;
       if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u') numberOfVowels++;
       else numberOfConsonants++;
      }
      if(numberOfConsonants==0)return 0;
      else return numberOfVowels/numberOfConsonants;    
    }
}
