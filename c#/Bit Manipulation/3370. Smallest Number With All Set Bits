      int answer=0;
      String binary= "";
      String largerBinary="";
      while(n>0){
       binary+=n%2;
       n/=2; 
      }
      for(int i=0;i<binary.Length;i++){
       char bit= binary[i];
       if(bit=='0')largerBinary+='1';
       else largerBinary+='1';
      }
      
      for(int i=binary.Length-1;i>=0;i--){
       int bit=binary[i];
       answer+=(int)Math.Pow(2,binary.Length-1-i);  
      }
      return answer;   
