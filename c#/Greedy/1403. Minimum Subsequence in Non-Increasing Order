public class Solution {
    public IList<int> MinSubsequence(int[] nums) {
      IList<int> list=new List<int>();
      Array.Sort(nums);
      long sum=0;
      long total=0;
      int index=nums.Length-1;
      for(int i=0;i<nums.Length;i++)sum+=1L*nums[i];
      while(total<=sum-total){
       total+=nums[index]; 
       list.Add(nums[index--]);
      }
      return list;  
    }
}
