public class Solution {
    public int LongestBalanced(string s) {
     int  answer=1;
     Dictionary<char,int> chars;
     for(int i=0;i<s.Length;i++){
      chars=new Dictionary<char,int>();  
      chars[s[i]]=1;  
      for(int j=i+1;j<s.Length;j++){
         if(!chars.ContainsKey(s[j]))chars[s[j]]=1;
         else chars[s[j]]=chars[s[j]]+1; 
         int numberOfFirst=chars[s[i]];
        int numberOfEqual=0;
        foreach(var(key,value)in chars){
            if(chars[key]==numberOfFirst)numberOfEqual++;  
        } 
        if(numberOfEqual==chars.Count)answer=Math.Max(answer,j-i+1); 
      }      
     }
     return answer;    
    }
}
