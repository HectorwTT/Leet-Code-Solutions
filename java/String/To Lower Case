class Solution {
    public String toLowerCase(String s) {
        String str="";
        for(int i=0;i<s.length();i++){
            char ch = s.charAt(i);
            if(ch>=65 && ch <=90){
                int chval1=ch+32;
                char ch1 =(char)chval1;
                str+=ch1;
            } else str+=ch;
        }
        return str;
    }
}
