class Solution {
    public String largestOddNumber(String num) {
        try{
         String result="";
         int oddMax=(int)Double.NEGATIVE_INFINITY;
         for(int i=0;i<num.length();i++){
          for(int j=i+1;j<=num.length();j++){
              String numval= num.substring(i,j);
              int value= Integer.parseInt(numval);
              if(value%2==1){
                  if(value>oddMax){
                      oddMax=value;
                      result= numval;
                  }
              }
          }
         }
         return result;
        }
        catch(Exception e){

        }
        return "";
    }
}
