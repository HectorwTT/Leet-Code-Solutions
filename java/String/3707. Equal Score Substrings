class Solution {
    public boolean scoreBalance(String s) {
      int [] sum=new int[s.length()];
      int total=0;
      for(int i=0;i<s.length();i++){
       char ch=s.charAt(i);
       int value=ch-96; 
       total+=value;
       sum[i]=total;
      }
      for(int i=0;i<s.length();i++){
       int prefix=sum[i];
       int suffix=total-sum[i];
       if(prefix==suffix) return true;
      }   
      return false;   
    }
}
