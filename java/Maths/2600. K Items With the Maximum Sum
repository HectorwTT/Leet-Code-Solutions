class Solution {
    public int kItemsWithMaximumSum(int numOnes, int numZeros, int numNegOnes, int k) {
        int maxSum=0;
        if(k<numOnes) return k;
        maxSum+=numOnes;
        k-=numOnes;
        if(k<numZeros) return maxSum;
        k-=numZeros;
        if(k<numNegOnes) return maxSum-k;
        else return maxSum-numNegOnes;
        
    }
}
