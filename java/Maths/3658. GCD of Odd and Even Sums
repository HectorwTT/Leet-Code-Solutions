class Solution {
    public int gcdOfOddEvenSums(int n) {
     int  sumOdd=1;
     int sumEven=2;
     int count=1;
     int odd=1;
     int even=2;
     while(count<n){
      odd+=2; 
      even+=2;System.out.println(odd);System.out.println(even);
      sumOdd+=odd;
      sumEven+=even;
      count++;
     }
     return gcd(sumOdd,sumEven);     
    }

    int gcd(int a, int b){
      if (b == 0) return a;
      return gcd(b, a % b);   
    }
}
