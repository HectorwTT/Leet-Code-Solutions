class Solution {
    public int[] applyOperations(int[] nums) {
     
     for(int i=1;i<nums.length;i++){
        if(nums[i]==nums[i-1]){
          nums[i-1]= 2*nums[i-1];
          nums[i]=0;
            
        } 
     } 
     moveZeroes(nums);
     return nums;
    }
     public void moveZeroes(int[] nums) {
        int newIndex=0;
        for(int i=0;i<nums.length;i++){
         if(nums[i]!=0){
            nums[newIndex++]=nums[i];
          
         }
        }
        for(int i=newIndex;i<nums.length;i++)nums[i]=0;
        return;
    }
}
