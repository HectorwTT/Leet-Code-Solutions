class Solution {
    public int[] sortArrayByParityII(int[] nums) {
        List<Integer> evens= new ArrayList<>();
        for(int i=0;i<nums.length;i++){
         if(nums[i]%2==0 && i%2!=0) evens.add(i);
        }
        for(int i=0;i<nums.length;i++){
         if(nums[i]%2==1 && i%2==0 && evens.size()!=0){
           int index= evens.get(0);
           evens.remove(0);
           int tmp= nums[i];
           int tmp1= nums[index];
           nums[index]=tmp;
           nums[i]= tmp1;
         }  
        }
        return nums;
    }
}
