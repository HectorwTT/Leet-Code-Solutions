class Solution {
    public int subarraySum(int[] nums) {
     int []  sums=new int[nums.length];
     int total=0;
     int subarraySum=0;
     for(int i=0;i<nums.length;i++){
      total+=nums[i];
      sums[i]=total; 
     }
     for(int i=0;i<nums.length;i++){
      int start=Math.max(0,i-nums[i]);
      int sum= 0;
      if(start==0)sum=sums[i];
      else sum=sums[i]-sums[start-1];
      subarraySum+=sum;  
     } 
     return subarraySum;     
    }
}
