class Solution {
    public int dominantIndices(int[] nums) {
      int []  sums=new int[nums.length];
      int total=0;
      int answer=0;
      for(int i=0;i<nums.length;i++){
       total+=nums[i];
       sums[i]=total; 
      }
      for(int i=0;i<nums.length-1;i++){
       int number=nums.length-(i+1);
       double average=(total-sums[i])/number;
       if(nums[i]>average) answer++;
      }
      return answer;
    }
}
