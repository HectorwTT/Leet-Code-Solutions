class Solution {
    public boolean threeConsecutiveOdds(int[] arr) {
     for(int i=2;i<arr.length;i++){
      int curr= arr[i];
      int previous1=arr[i-1];
      int previous2=arr[i-2];
      if(curr%2==1 && previous1%2==1 && previous2%2==1)return true;  
     }
     return false;        
    }
}
