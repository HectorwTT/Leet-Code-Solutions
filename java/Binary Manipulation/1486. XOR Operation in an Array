class Solution {
    public int xorOperation(int n, int start) {
       int [] nums= new int[n];
       int number =start;
       for(int i=0;i<nums.length;i++){
           nums[i]=start+(2*i);
           if(i>=1){
               number ^= nums[i];
           }
       } 
       return number;
    }
}
