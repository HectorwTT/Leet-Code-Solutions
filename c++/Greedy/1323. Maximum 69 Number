class Solution {
public:
    int maximum69Number (int num) {
     string str="";
     int numOfSixes=0; 
     string number=to_string(num);
     int answer=0;
     for(int i=0;i<number.size();i++){
      char ch=number[i];
      if(ch=='9' || numOfSixes==1)str+=ch;
      else {
       str+="9";
       numOfSixes++;
      }
     }
     for(int i=str.size()-1;i>=0;i--){
      int power=str.size()-1-i;
      int chVal=str[i]-48;
      answer+=chVal*std::pow(10,power); 
     }
     return answer;     
    }
};
