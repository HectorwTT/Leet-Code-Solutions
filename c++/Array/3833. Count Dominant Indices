class Solution {
public:
    int dominantIndices(vector<int>& nums) {
      vector<int> sums;
      int total=0;
      int answer=0;
      for(int i=0;i<nums.size();i++){
       total+=nums[i];
       sums.push_back(total); 
      }
      for(int i=0;i<nums.size()-1;i++){
       int number=nums.size()-(i+1);
       double average=(total-sums[i])/number;
       if(nums[i]>average) answer++;
      }
      return answer;   
    }
};
