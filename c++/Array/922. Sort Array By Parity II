class Solution {
public:
    vector<int> sortArrayByParityII(vector<int>& nums) {
      vector<int> answer;
      int evenIndex=0;
      int oddIndex=1;
      for(int i=0;i<nums.size();i++)answer.push_back(0);
      for(int i=0;i<nums.size();i++){
        if(nums[i]%2==0){
            answer[evenIndex]=nums[i];
            evenIndex+=2;
        }else{
          answer[oddIndex]=nums[i];
          oddIndex+=2;  
        }    
      }
      return answer; 
    }
};
