class Solution {
public:
    int triangularSum(vector<int>& nums) {
     vector<int> list;
     for(int i=0;i<nums.size();i++)list.push_back(nums[i]);
     while(list.size()!=1){
      vector<int> newList;
      for(int i=0;i<list.size()-1;i++){
       int total=(list[i]+list[i+1])%10;
       newList.push_back(total);
      } 
      list=newList;
     } 
     return list[0];  
    }
};
