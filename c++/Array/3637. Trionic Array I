class Solution {
public:
    bool isTrionic(vector<int>& nums) {
      int p=-1;
      int q=-1;      
      for(int i=1;i<nums.size();i++){
        if(nums[i]==nums[i-1])return false;
        else if(nums[i]<nums[i-1])break;
        else p=i;
      } 
      if(p==-1)return false;
      for(int i=p+1;i<nums.size();i++){
        if(nums[i]==nums[i-1])return false;
        else if(nums[i]>nums[i-1])break;
        else q=i;
      } if(q==-1 || q==nums.size()-1)return false;
      for(int i=q+1;i<nums.size();i++){
        if(nums[i]<=nums[i-1])return false;
      }
      return p!=-1 && q!=-1;   
    }
};
