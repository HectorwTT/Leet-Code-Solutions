class Solution {
public:
    int minOperations(vector<int>& nums) {
     unordered_map<int,int>map;
     for(int i=0;i<nums.size();i++){
      if(map.find(nums[i])!=map.end())map[nums[i]]=1;
      else map[nums[i]]=map[nums[i]]+1;  
     }  
      if(map.size()==1) return 0;
      else return 1;   
    }
};
