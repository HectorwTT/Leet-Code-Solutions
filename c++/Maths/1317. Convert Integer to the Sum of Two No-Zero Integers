class Solution {
public:
    vector<int> getNoZeroIntegers(int n) {
      vector<int>answer;
      int first  =1;
      int second  =n-1;
      string firstValue=to_string(first);
      string secondValue=to_string(second); 
      while(secondValue.find('0')!=-1 || firstValue.find('0')!=-1){
        first++;
       second--;
       firstValue=to_string(first);
       secondValue=to_string(second); 
      }
      answer.push_back(first);
      answer.push_back(second);
      return answer;    
    }
};
