class Solution {
public:
    vector<int> recoverOrder(vector<int>& order, vector<int>& friends) {
     vector<int> answer;
     unordered_map<int,int> map;
     for(int i=0;i<friends.size();i++){
       map[friends[i]]=1; 
     }  
     for(int i=0;i<order.size();i++){
      if(map.find(order[i])!=map.end())answer.push_back(order[i]) ; 
     }
     return answer;   
    }
};
