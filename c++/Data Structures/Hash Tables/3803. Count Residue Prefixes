class Solution {
public:
    int residuePrefixes(string s) {
      unordered_map<char,int> map;
      int answer=0;
      for(int i=0;i<s.size();i++){
       char ch=s[i];
       if(map.find(ch)==map.end())map[ch]=1;
       else map[ch]=map[ch]+1;
       if(map.size()==(i+1)%3)answer++;
      } 
      return answer;     
    }
};
