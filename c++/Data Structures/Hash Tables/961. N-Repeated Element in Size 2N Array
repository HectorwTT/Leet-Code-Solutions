class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
     unordered_map<int,int> map;
     for(int i=0;i<nums.size();i++){
      if (map.find(nums[i])==map.end())map[nums[i]]=1;
      else map[nums[i]]=map[nums[i]]+1; 
     }
     for(auto key:map){
      if(key.second==nums.size()/2)return key.first;  
     }
     return -1;     
    }
};
