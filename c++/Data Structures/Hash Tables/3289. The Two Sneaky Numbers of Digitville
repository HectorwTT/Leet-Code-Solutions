class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
      vector<int> answer;
      unordered_map<int,int> map;
      for(int i=0;i<nums.size();i++){
       if(map.find(nums[i])!=map.end()){
        answer.push_back(nums[i]);
        if(answer.size()==2)break;
       }else map[nums[i]]=1;        
      }
      return answer;     
    }
};
