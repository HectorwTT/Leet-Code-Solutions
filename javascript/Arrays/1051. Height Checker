var heightChecker = function(heights) {
 const sortedHeights=[];
 sortedHeights.length=heights.length;
 for(let i=0;i<sortedHeights.length;i++)sortedHeights[i]=heights[i];
 sortedHeights.sort(function(a, b){return a - b});
 let total=0;
 for(let i=0;i<sortedHeights.length;i++){
  if(sortedHeights[i]!= heights[i])total++;console.log(sortedHeights[i]+"");
 }
 return total;   
};
