var sortArrayByParityII = function(nums) {
      const answer=[];
      let evenIndex=0;
      let oddIndex=1;
      for(let i=0;i<nums.length;i++)answer.push(0);
      for(let i=0;i<nums.length;i++){
        if(nums[i]%2==0){
            answer[evenIndex]=nums[i];
            evenIndex+=2;
        }else{
          answer[oddIndex]=nums[i];
          oddIndex+=2;  
        }    
      }
      return answer;   
};
