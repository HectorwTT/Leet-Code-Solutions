var largestEven = function(s) {
      let numberOfTwos=0;
      let lastTwoIndex=0;
      var ch='0';
      for(let i=0;i<s.length;i++){
       ch=s.charAt(i);
       if(ch=='2'){
        numberOfTwos++; 
        lastTwoIndex=i;
       } 
      }
      if(numberOfTwos==0) return "";
      else{
       if(ch=='1') return s.substring(0,lastTwoIndex+1);
       else return s;  
      }  
};
