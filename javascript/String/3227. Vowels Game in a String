var doesAliceWin = function(s) {
     let numOfVowels=0;
     let numOfConsonants=0;
     for(let i=0;i<s.length;i++){
      var ch = s.charAt(i);
      if(ch=='a' || ch =='e' || ch=='i' || ch=='o' || ch=='u')numOfVowels++;
      else numOfConsonants++;  
     }
     if(numOfVowels==0) return false;
     else return true;    
};
